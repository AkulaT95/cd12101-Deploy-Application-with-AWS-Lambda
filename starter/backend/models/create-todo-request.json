{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todoCreate",
  "type": "object",
  "properties": {
    "todoId": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "attachmentUrl": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "done": {
      "type": "boolean"
    }
  },
  "required": ["name", "dueDate"],
  "additionalProperties": false
}